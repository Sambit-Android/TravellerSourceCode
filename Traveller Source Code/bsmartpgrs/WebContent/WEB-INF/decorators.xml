<?xml version="1.0" encoding="ISO-8859-1"?>
<decorators defaultdir="/WEB-INF/decorators">
	
	<decorator name="loginpage" page="loginlayout.jsp">
		<pattern>/</pattern>
	</decorator>
	
	<decorator name="login" page="loginlayout.jsp">
		<pattern>/login</pattern>
	</decorator>
	
	<decorator name="registerTicket" page="loginlayout.jsp">
